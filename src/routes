const {Router} = require("express")
const routes = Router()


let animal;

routes.post('/nuevo-animal',(req, res)=>{
    animal = req.body

    if(!animal.name){
        res.status(401).json({data: animal, messaje: "Todos los campos son nesesarios"})
    }

    res.status(200).json({data: animal, messaje: "animal cambiado"})
});



module.exports = routes
